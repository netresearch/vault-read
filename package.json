      "-v"
    ],
    "commands": "./lib",
    "bin": "vault-read"
  },
  "repository": "netresearch/vault-read",
  "scripts": {
    "postinstall": "sh scripts/postinstall.sh",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "build:lib": "tsc",
    "prepare": "husky"
  },
  "types": "lib/index.d.ts",
  "resolutions": {
    "got": "14.6.6"
  }
}
